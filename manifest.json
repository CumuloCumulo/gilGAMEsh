{
  "manifest_version": 3,
  "name": "语雀增强导出",
  "version": "2.0.0",
  "description": "一键导出语雀文档和视频到Obsidian",
  "permissions": [
    "downloads",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.yuque.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.yuque.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
